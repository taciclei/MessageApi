{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
